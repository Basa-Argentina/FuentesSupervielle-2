package com.security.exceptions;

public class ReposicionamientoException extends BasaException {
	private static final long serialVersionUID = 1123L;

	public ReposicionamientoException() {
		super("Ha ocurrido un error durante el reposicionamiento");
	}

	public ReposicionamientoException(String message, Throwable cause) {
		super(message, cause);
	}

	public ReposicionamientoException(String message) {
		super(message);
	}

	public ReposicionamientoException(Throwable cause) {
		super("Ha ocurrido un error durante el reposicionamiento", cause);
	}
}
